{
  "name": "safe-haven",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.11.1"
  },
  "scripts": {
    "clean": "rimraf public",
    "start": "pm2 reload --attach ./build-scripts/pm2_www.config.js && pm2 logs all",
    "postinstall": "yarn prod:build",
    "start:dev": "yarn test && NODE_ENV=development nodemon ./build-scripts/www --config",
    "start:debug": "NODE_ENV=debug nodemon --inspect ./server/scripts/www --config",
    "start:prod": "pm2 reload --attach ./build-scripts/pm2_www.config.js && pm2 logs all",
    "lint:server": "./node_modules/.bin/eslint --ext js server",
    "prod:build": "rimraf public && webpack --progress --config webpack/webpack.config.babel.js",
    "test": "NODE_ENV=test PROCESS_TYPE=web ./node_modules/.bin/mocha test/setup.js 'server/**/*.test.js'",
    "heroku-prebuild": "yarn global add pm2"
  },
  "dependencies": {
    "async": "^2.5.0",
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "~1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "chai": "^4.1.0",
    "cheerio": "~1.0.0-rc.2",
    "contentful": "^4.5.0",
    "cookie-parser": "~1.4.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "dompurify": "^0.9.0",
    "dotenv": "~4.0.0",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "express": "~4.15.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.5",
    "express-status-monitor": "^0.1.9",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs": "^0.0.1-security",
    "handlebars": "^4.0.10",
    "handlebars-helpers": "^0.8.2",
    "handlebars-loader": "^1.5.0",
    "hbs": "~4.0.1",
    "helmet": "^3.8.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "jquery": "^3.2.1",
    "jshint": "~2.9.5",
    "lodash": "^4.17.4",
    "mixitup": "^3.2.1",
    "mocha": "^3.4.2",
    "moment": "~2.18.1",
    "mongoose": "^4.11.6",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "parsleyjs": "^2.7.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.1.0",
    "pm2": "^2.6.1",
    "postcss-loader": "^2.0.6",
    "raven": "^2.1.0",
    "request": "~2.81.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "showdown": "^1.7.1",
    "sinon": "^2.3.8",
    "sinon-chai": "^2.12.0",
    "slick-carousel": "^1.6.0",
    "style-loader": "^0.18.2",
    "tether": "^1.4.0",
